.bk {
  width: 95%;
  margin: 30rpx auto;
}
.bk image {
  width: 100%;
}
.box {
  width: 95%;
  box-sizing: border-box;
  background: rgba(30, 43, 73, 0.8);
  border: 1px solid #5d72a4;
  border-radius: 6rpx;
  margin: 20rpx auto;
}
.box .head {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1px solid #5d72a4;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  font-size: 27rpx;
  color: #2DEAB7;
}
.box .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1px solid rgba(93, 114, 164, 0.5);
  box-sizing: border-box;
  padding: 30rpx 30rpx;
  font-size: 27rpx;
  color: #90A3C6;
  border-radius: 10rpx;
}
.box .item .num {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.box .item .num .radio {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  border: 1px solid #91A4C6;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
