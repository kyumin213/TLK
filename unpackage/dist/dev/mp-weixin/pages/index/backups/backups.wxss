.backups {
  height: 100vh;
  background-image: url(/static/bg.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.backups .backups-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.backups .backups-box .backups-top {
  width: 100%;
  font-size: 30rpx;
  color: #fff;
  box-sizing: border-box;
  padding-left: 75rpx;
  margin-top: 52rpx;
}
.backups .push-box {
  width: 680rpx;
  height: 360rpx;
  background-color: none;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding-right: 10rpx;
  padding-top: 5rpx;
}
.backups .push-box > view:nth-of-type(3n+1) {
  margin-left: 41rpx;
}
.backups .list {
  width: 160rpx;
  height: 50rpx;
  background-color: none;
  box-sizing: border-box;
  background: rgba(18, 25, 42, 0.5);
  border: #2DE9B7 1rpx solid;
  border-radius: 25rpx;
  font-size: 24rpx;
  color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 30rpx;
  margin-left: 60rpx;
  float: left;
}

