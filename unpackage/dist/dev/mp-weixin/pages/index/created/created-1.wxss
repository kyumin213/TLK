  height: 100vh;
  background-image: url(/static/bg.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.created-1 .created-lsit {
  height: 100rpx;
  background-color: #1D2B49;
  font-size: 24rpx;
  color: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
